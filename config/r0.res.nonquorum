resource r0 {
	disk /dev/sdb;
	device /dev/drbd0;
	meta-disk internal;
    net {
        allow-two-primaries no;
        after-sb-0pri discard-zero-changes;
        after-sb-1pri discard-secondary;
        after-sb-2pri disconnect;
        rr-conflict disconnect;
    }
    on NODE1 {
        address  NODE1_IP:7789;
    }
    on NODE2 {
        address  NODE2_IP:7789;
    }
}
